{
  "name": "rejection",
  "version": "1.0.0",
  "description": "a module to create better error objects",
  "main": "src/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "babel-node ./node_modules/mocha/bin/_mocha --recursive ./test/unit",
    "test-debug": "babel-node --inspect --debug-brk ./node_modules/mocha/bin/_mocha --recursive ./test/unit",
    "cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --recursive ./test/unit",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "eslint ./src",
    "clean": "rimraf ./lib",
    "build": "npm run lint && npm run clean && babel ./src --out-dir ./lib",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeMedic42/rejection.git"
  },
  "author": "CodeMedic42",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodeMedic42/rejection/issues"
  },
  "homepage": "https://github.com/CodeMedic42/rejection#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.8.0",
    "istanbul": "^1.0.0-alpha.2",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "lodash": "^4.17.4",
    "single-space": "^1.0.0"
  }
}
